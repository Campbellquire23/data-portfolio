   "source": [
    "# Dictionary of sales by person\n",
    "sales = {\n",
    "    \"Alex\": 250,\n",
    "    \"Sam\": 300,\n",
    "    \"Jamie\": 200\n",
    "}\n",
    "\n",
    "# Total all sales\n",
    "total_sales = sum(sales.values())\n",
    "\n",
    "# Print each and total\n",
    "for person, amount in sales.items():\n",
    "    print(f\"{person}: ${amount}\")\n",
    "\n",
    "print(f\"\\nTotal sales: ${total_sales}\")\n"
   ]
