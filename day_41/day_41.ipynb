   "source": [
    "# Weekly sales by employee\n",
    "sales_data = {\n",
    "    \"Alex\": [200, 150, 180],\n",
    "    \"Jamie\": [120, 130, 140],\n",
    "    \"Morgan\": [300, 290]\n",
    "}\n",
    "\n",
    "# Print totals\n",
    "for person, sales in sales_data.items():\n",
    "    total = sum(sales)\n",
    "    print(f\"{person} made ${total} in total sales.\")\n"
   ]
